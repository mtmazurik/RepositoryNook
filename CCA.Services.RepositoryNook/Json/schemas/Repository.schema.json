{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://cloudcomputingassociates.com/schemas/Repository.schema.json",
  "type": "object",
  "title": "Repository",
  "required": [
    "_id",
    "keyName",
    "keyValue"
    "app",
    "objectSchemaName",
    "objectSchemaUri",
    "objectType",
    "objectData"
  ],
  "properties": {
    "_id": {
      "$id": "#/properties/_id",
      "type": "null",
      "title": "The _id Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "keyName": {
      "$id": "#/properties/keyName",
      "type": "string",
      "title": "The keyName Schema",
      "default": "",
      "examples": [
        "flightNumber"
      ],
      "pattern": "^(.*)$"
    },
    "keyValue": {
      "$id": "#/properties/keyValue",
      "type": "string",
      "title": "The keyValue Schema",
      "default": "",
      "examples": [
        "580"
      ],
      "pattern": "^(.*)$"
    },
    "tags": {
      "$id": "#/properties/tags",
      "type": "array",
      "title": "The Tags Schema",
      "items": {
        "$id": "#/properties/tags/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "777x",
          "Boeing"
        ],
        "pattern": "^(.*)$"
      }
    },
    "createdDate": {
      "$id": "#/properties/createdDate",
      "type": "null",
      "title": "The Createddate Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "createdBy": {
      "$id": "#/properties/createdBy",
      "type": "string",
      "title": "The Createdby Schema",
      "default": "",
      "examples": [
        "mtmazurik"
      ],
      "pattern": "^(.*)$"
    },
    "modifiedDate": {
      "$id": "#/properties/modifiedDate",
      "type": "null",
      "title": "The Modifieddate Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "modifiedBy": {
      "$id": "#/properties/modifiedBy",
      "type": "null",
      "title": "The Modifiedby Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "app": {
      "$id": "#/properties/app",
      "type": "string",
      "title": "The App Schema",
      "default": "",
      "examples": [
        "FlightBook"
      ],
      "pattern": "^(.*)$"
    },
    "repository": {
      "$id": "#/properties/repository",
      "type": "string",
      "title": "The Repository Schema",
      "default": "",
      "examples": [
        "repository-nook-db"
      ],
      "pattern": "^(.*)$"
    },
    "collection": {
      "$id": "#/properties/collection",
      "type": "string",
      "title": "The Collection Schema",
      "default": "",
      "examples": [
        "flights"
      ],
      "pattern": "^(.*)$"
    },
    "schemaName": {
      "$id": "#/properties/schemaName",
      "type": "string",
      "title": "The schemaName Schema",
      "default": "",
      "examples": [
        "TicketingData"
      ],
      "pattern": "^(.*)$"
    },
    "schemaUri": {
      "$id": "#/properties/schemaUri",
      "type": "string",
      "title": "The schemaUri Schema",
      "default": "",
      "examples": [
        "http://cloudcomputingassociates/schemas/TicketingData.json.schema"
      ],
      "pattern": "^(.*)$"
    },
    "data": {
      "$id": "#/properties/dataata",
      "type": "string",
      "title": "The data Schema",
      "default": "",
      "examples": [
        "{ \"ticketingData\" : { \"flightNumber\" : \"580\", \"dayOfWeek\" : \"Monday\",\"manufacturer\":\"Boeing\", \"planeType\":\"777x\" }}"
      ],
      "pattern": "^(.*)$"
    }
  }
}